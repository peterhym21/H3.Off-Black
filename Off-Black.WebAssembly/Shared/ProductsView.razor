
@if (ProductDTOs != null)
{
    @foreach (var item in ProductDTOs)
    {
        <div class=" col-6">
            <a href="/ProdcutDetails/@item.ProductID">
                <img src="@item.ProductImagePath" alt="CategoryImage" height="400" />
                <h5>@item.ProductName</h5>
                <h5>@item.Price Kr.</h5>
            </a>
            <br />
        </div>
    }
}



@code {
    [Parameter]
    public List<ProductDTO> ProductDTOs { get; set; }

}
